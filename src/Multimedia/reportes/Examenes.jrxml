     examenvehiculo.`numExamenVeh` AS examenvehiculo_numExamenVeh,
     examenvehiculo.`nombExaminador` AS examenvehiculo_nombExaminador,
     examenvehiculo.`dominioVeh` AS examenvehiculo_dominioVeh,
